<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

    <mapper namespace="com.fh.dao.UserDao">

    <select id="queryUserByUsername" parameterType="java.lang.String"  resultType="com.fh.model.bean.UserBean">
        select * from t_users where username = #{username}
    </select>


</mapper>